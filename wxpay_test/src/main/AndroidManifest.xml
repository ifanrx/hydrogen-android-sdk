<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.minapp.android.example.wechat.pay">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
            android:name=".App"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>


        <activity
                android:name="com.minapp.android.sdk.wechat.WXPayEntryActivity"
                android:theme="@style/HyDrogenTranslucent"
                android:launchMode="singleTop"
        />

        <activity-alias
                android:exported="true"
                android:name=".wxapi.WXPayEntryActivity"
                android:targetActivity="com.minapp.android.sdk.wechat.WXPayEntryActivity"
        />
        <activity android:name="com.minapp.android.sdk.alipay.AlipayActivity"
                  android:theme="@style/HyDrogenTranslucent"
                  android:launchMode="singleTop"
                  />
    </application>

</manifest>